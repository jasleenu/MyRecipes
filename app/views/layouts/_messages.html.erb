<div class="row">
  <div class = "col-md-12">
    <br>
    <% flash.each do |message_type, message| %>
      <div class ="alert alert-dismissable alert-<%=message_type%>"><%=message%>
       <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
      </div>
      <%end%>
  </div>
</div>